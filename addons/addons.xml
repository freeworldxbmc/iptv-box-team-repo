<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.video.iptvbox.tv"
       name="IPTVBoxTV"
       version="1.0.1"
       provider-name="IPTV Box Team">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
    <import addon="script.module.beautifulsoup" version="3.2.1"/>
    <import addon="script.module.simple.downloader" version="0.9.4"/>
    <import addon="script.module.simplejson" />
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary>IPTV Box TV</summary>
    <description>
        SOMOS IPTV BOX TV TEAM DISFRUTA NUESTRO SERVICIO!!
    </description>
    <platform>all</platform>
  </extension>
</addon>

<addon id="plugin.video.latinTester" version="1.0.3" name="latinTester" provider-name="Medieval">
    <requires>
        <import addon="xbmc.python" version="2.1.0"/>
        <import addon="script.video.LatinProxy" version="1.0.0"/>
    </requires>
    <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>video</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>latinTester</summary>
        <description></description>
        <platform>all</platform>
    </extension>
</addon>

<addon id="plugin.video.veetle" version="0.3.1" name="Veetle" provider-name="fightnight (creator: t0mm0)">
    <requires>
        <import addon="xbmc.python" version="2.1.0"/>
        <import addon="script.module.simplejson" version="2.0.10"/>
        <import addon="script.common.plugin.cache" version="1.3.0"/>
    </requires>
    <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>video</provides>
    </extension>
    <extension point= "xbmc.addon.metadata">
        <summary lang="el">Αναζήτηση και ρεύμα από veetle.com</summary>
		<summary lang="en">Browse and stream from veetle.com</summary>
		<summary lang="pt">Veja canais de veetle.com</summary>
		<summary lang="pt_BR">Veja canais de veetle.com</summary>
        <description lang="el">Επιτρέπει βλέποντας ρέματα veetle.com που flash ενεργοποιηθεί.</description>
		<description lang="en">Enables watching streams on veetle.com that are flash enabled.</description>
		<description lang="pt">Permite visualizar streams de veetle.com que necessitam de flash.</description>
		<description lang="pt_BR">Permite visualizar streams de veetle.com que necessitam de flash.</description>
		<disclaimer lang="el">Δεν λειτουργεί για τις ροές 'HD' που απαιτούν λήψη λογισμικού.</disclaimer>
        <disclaimer lang="en">Does not work for 'HD' streams which require a software download.</disclaimer>
		<disclaimer lang="pt">Não funciona para streams 'HD' que necessitam do download de software.</disclaimer>
		<disclaimer lang="pt_BR">Não funciona para streams 'HD' que necessitam do download de software.</disclaimer>
        <platform>all</platform>
    </extension>
</addon>

<addon id="repository.iptvboxtv-addons" name="IPTV Box Team's Repo" version="1.0.0" provider-name="IPTV Box Team">
	<extension point="xbmc.addon.repository" name="IPTV Box Team's Repo">
		<info compressed="false">https://iptv-box-team-repo.googlecode.com/svn/addons/addons.xml</info>
		<checksum>https://iptv-box-team-repo.googlecode.com/svn/addons/addons.xml.md5</checksum>
		<datadir zip="true">https://iptv-box-team-repo.googlecode.com/svn/addons</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>IPTV Box Teams's repo</summary>
   		<description lang="en">Instala los plugins de IPTV Box Repo.</description>
		<disclaimer></disclaimer>
		<platform>all</platform>
	</extension>
</addon>

<addon id="script.video.LatinProxy"
	     name="LatinProxy"
	     version="1.0.3"
	     provider-name="Medieval">
  <requires>
    <import addon="xbmc.python" version="2.1.0" />
  </requires>
  <extension point="xbmc.python.module" library="lib" />
    <extension point="xbmc.python.pluginsource" library="addon.py">
		<provides>executable</provides>
  </extension>

  <extension point="xbmc.addon.metadata">
    <platform>all</platform>
    <summary lang="en">Proxy for f4m</summary>
    <description lang="en">Use this addon to play F4m v1 and v2 streams</description>
  </extension>
</addon>
</addons>
